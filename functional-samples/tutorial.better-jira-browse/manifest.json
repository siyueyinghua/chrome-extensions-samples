{
  "manifest_version": 3,
  "name": "Better Jira Browse",
  "description": "Provide the ability to improve User Experience on Jira browse page, including toggle visibility of right hand sidebar or panel, and maxmize the Create Issue pop up dialog or window.",
  "version": "0.2",
  "author": "yemingguang_hit@163.com", 
  "homepage_url": "https://github.com/siyueyinghua/chrome-extensions-samples/tree/main/functional-samples/tutorial.better-jira-browse/README.md",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_title": "Click to toggle right hand sidebar/panel of Jira browse. Shortcut: Ctrl+B",
    "default_icon": {
      "16": "images/icon_off-16.png",
      "32": "images/icon_off-32.png",
      "48": "images/icon_off-48.png",
      "128": "images/icon_off-128.png"
    }
  },
  "permissions": ["activeTab", "scripting", "storage"],
  "background": {
    "service_worker": "service-worker.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  }
}