{
  "manifest_version": 3,
  "name": "Better Jira Browse",
  "description": "Improve User Experience on Jira browse page, toggle right hand sidebar, maxmize the Create Issue pop-up dialog.",
  "version": "0.3",
  "author": "yemingguang_hit@163.com", 
  "homepage_url": "https://github.com/siyueyinghua/chrome-extensions-samples/tree/main/functional-samples/tutorial.better-jira-browse-more-debugging/README.md",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_title": "Click to toggle right hand sidebar/panel of Jira browse. Shortcut: Ctrl+B",
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "service-worker.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  }
}