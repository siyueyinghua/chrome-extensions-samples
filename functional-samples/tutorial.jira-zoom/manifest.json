{
    "manifest_version": 3,
    "name": "img-zoom",
    "version": "1.0",
  
    "description": "Zoom jira's image",
  
    "icons": {
      "48": "icons/border-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://jira.mthreads.com/*"],
        "js": ["zoom.js"],
        "run_at": "document_idle"
      }
    ]
  }